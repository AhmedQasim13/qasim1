                                    الرقم التسلسلي
                                    (ID)
                                    :
                                </label>
                                <input asp-for=StId type="text" class="form-control" id="id">
                            </div>
                            <button  type="submit" class="theme-btn style-ten"> تسجيل الدخول </button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
      </section> 
          <!-- js file  -->
    <script src="~/js/jquery-3.6.0.min.js"></script>
    <script src="~/js/bootstrap5rtl.js"></script>

    <!-- Custom js -->
    <script src="~/js/custom.js"></script>
