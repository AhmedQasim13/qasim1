
<!DOCTYPE html>
<html lang="ar" dir="rtl">
@{
    ViewData["Title"] = "Home Page";
}
@model Student
<head>

</head>

 <body>
    <section class="bg-form-result pt-5 pb-5" style="background-image:url(../images/banner_bg.jpg); height:100vh;" >
        <div class="container">
            <div class="row align-items-center justify-content-center">
                <div class="col-lg-7 col-sm-12">
                    <div class="form-box ">
                        <!-- title  -->
                        <div class="image-logo text-center">
                            <img src="~/images/logoo.png" alt="" width="130px">
